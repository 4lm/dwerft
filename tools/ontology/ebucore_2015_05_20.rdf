  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/contributor">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="#BusinessObject"/>
          <rdfs:Class rdf:about="#Resource"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:comment xml:lang="en">An Agent who has participated in any phase of management of a BusinessObject or Resource.</rdfs:comment>
    <rdfs:label xml:lang="en">Contributor</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="subtitlingSource">
    <rdfs:domain rdf:resource="#Subtitling"/>
    <rdfs:label xml:lang="en">Subtitling source</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment xml:lang="en">To identify the source of the Subtitling
            resource.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="contactFamilyInformation">
    <rdfs:comment xml:lang="en">Information on the family of a Contact /
            person.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Contact"/>
    <rdfs:label xml:lang="en">Family information</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="hasTargetAudience">
    <rdfs:comment xml:lang="en">To associate a TargetAudience (e.g. for
            parental guiddance or targeting a particular social group) with a
            BusinessObject/Resource.</rdfs:comment>
    <rdfs:domain rdf:resource="#BusinessObject"/>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#hasTargetAudience"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Target audience</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="#hasType"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.w3.org/ns/ma-ont#isRelatedTo">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >To define relations as defined in the W3C media ontology (ma-ont)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Relation</rdfs:label>
    <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Relation</skos:prefLabel>
  </rdf:Property>
  <rdf:Property rdf:ID="hasPublicationEvent">
    <rdfs:range rdf:resource="#PublicationEvent"/>
    <rdfs:label xml:lang="en">Publication event</rdfs:label>
    <rdfs:comment xml:lang="en">To associate PublicationEvents with
            PublicationChannels or as elements of a PublicationHistory or PublicationPlanning.</rdfs:comment>
