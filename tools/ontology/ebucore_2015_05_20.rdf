    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Contact"/>
    <rdfs:label xml:lang="en">Education</rdfs:label>
  </rdf:Property>
