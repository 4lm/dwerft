    <rdfs:comment xml:lang="en">To identify the Agent (Contact/person or
            Organisation) who owns a Service operating a PublicationChannel.</rdfs:comment>
    <rdfs:domain rdf:resource="#Service"/>
    <rdfs:range rdf:resource="#Agent"/>
    <rdfs:label xml:lang="en">Owner</rdfs:label>
    <rdfs:subPropertyOf>
      <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/relation"/>
    </rdfs:subPropertyOf>
  </rdf:Property>
  <rdf:Property rdf:about="http://purl.org/dc/elements/1.1/rights">
    <rdfs:label xml:lang="en">Rights</rdfs:label>
    <rdfs:comment xml:lang="en">Rights information related to an Asset, aPublicationEvent, a BusinessObject or a resource.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="#Asset"/>
          <rdfs:Class rdf:about="#PublicationEvent"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </rdf:Property>
  <rdf:Property rdf:ID="SDID">
    <rdfs:domain rdf:resource="#AncillaryData"/>
