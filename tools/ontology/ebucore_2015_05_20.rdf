    <rdfs:subPropertyOf rdf:resource="#hasFormat"/>
  </rdf:Property>
  <rdf:Property rdf:ID="loudnessIntegratedLoudness">
    <rdfs:subPropertyOf rdf:resource="#loudnessParameters"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
