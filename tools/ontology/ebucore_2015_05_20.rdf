    <rdfs:subPropertyOf rdf:resource="#officeMailAddress"/>
    <rdfs:label xml:lang="en">Country</rdfs:label>
    <rdfs:comment xml:lang="en">To provide the country name and or country
            code.</rdfs:comment>
  </rdf:Property>
