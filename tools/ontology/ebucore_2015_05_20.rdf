      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="#BusinessObject"/>
          <rdfs:Class rdf:about="#Annotation"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#hasKeyword"/>
