      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="#PublicationChannel"/>
          <rdfs:Class rdf:about="#PublicationHistory"/>
          <rdfs:Class rdf:about="#PublicationPlanning"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </rdf:Property>
  <rdf:Property rdf:ID="hasTheme">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
