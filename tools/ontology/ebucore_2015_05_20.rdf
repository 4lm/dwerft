    <rdfs:subPropertyOf rdf:resource="#hasKeyword"/>
  </rdf:Property>
  <rdf:Property rdf:ID="frameRate">
    <rdfs:domain rdf:resource="#VideoTrack"/>
    <rdfs:label xml:lang="en">Frame rate</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:comment xml:lang="en">The frame rate of the video signal in frame per
            second.</rdfs:comment>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#frameRate"/>
  </rdf:Property>
  <rdf:Property rdf:ID="lounessMaxShortTerm">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="#AudioContent"/>
          <rdfs:Class rdf:about="#AudioProgramme"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:subPropertyOf rdf:resource="#loudnessParameters"/>
    <rdfs:comment xml:lang="en">The maximum short term loudness measured at AudioProgramme or AudioContent level.</rdfs:comment>
    <rdfs:label xml:lang="en">Max short term loudness</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </rdf:Property>
  <rdf:Property rdf:about="http://purl.org/dc/terms/modified">
    <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/elements/1.1/date"/>
    <rdfs:label xml:lang="en">Modified</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:about="http://www.w3.org/ns/ma-ont#averageBitRate">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Average bit rate</rdfs:label>
    <skos:prefLabel rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Average bit rate</skos:prefLabel>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >The average bit rate as defined in the media annotation ontology (ma-ont) http://www.w3.org/TR/mediaont-10/</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="organisationName">
    <rdfs:comment xml:lang="en">To provide the full name of an Organisation.</rdfs:comment>
