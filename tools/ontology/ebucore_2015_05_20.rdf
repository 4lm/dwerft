    <rdfs:label xml:lang="en">Name</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="packageName">
    <rdfs:domain rdf:resource="#Resource"/>
    <rdfs:label xml:lang="en">Package name</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
    <rdfs:comment xml:lang="en">The name attributed to a
            package.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="hasPart">
    <rdfs:subPropertyOf rdf:resource="#hasRelatedBusinessObject"/>
    <rdfs:range rdf:resource="#EditorialObject"/>
