  <rdf:Property rdf:ID="locationLongitude">
    <rdfs:comment xml:lang="en">To define the longitude of the
            Location.</rdfs:comment>
    <rdfs:domain rdf:resource="#Location"/>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#locationLongitude"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    <rdfs:label xml:lang="en">Longitude</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="partName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Part"/>
    <rdfs:comment xml:lang="en">A name by which the Part is identified.</rdfs:comment>
    <rdfs:label xml:lang="en">Part name</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="noiseFilter">
    <rdfs:comment xml:lang="en">A flag to signal that a noise filter has been
            used.</rdfs:comment>
    <rdfs:domain rdf:resource="#VideoFormat"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label xml:lang="en">Noise filter</rdfs:label>
  </rdf:Property>
  <rdf:Property rdf:ID="isCopyrightedBy">
    <rdfs:label xml:lang="en">Copyright</rdfs:label>
    <rdfs:subPropertyOf>
      <rdf:Property rdf:ID="hasRights"/>
    </rdfs:subPropertyOf>
    <owl:equivalentProperty rdf:resource="http://www.w3.org/ns/ma-ont#isCopyrightedBy"/>
    <rdfs:comment xml:lang="en">Copyright statement.</rdfs:comment>
  </rdf:Property>
  <rdf:Property rdf:ID="officeAddressLocality">
    <rdfs:subPropertyOf>
      <rdf:Property rdf:about="#officeMailAddress"/>
    </rdfs:subPropertyOf>
    <rdfs:label xml:lang="en">Locality</rdfs:label>
    <rdfs:comment xml:lang="en">To provide the name of a city, village,
            etc.</rdfs:comment>
  </rdf:Property>
