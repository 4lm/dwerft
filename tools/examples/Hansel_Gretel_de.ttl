        foo:hasScene              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/63e11434-45fa-4e34-813a-d5087e1093fc> , <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/f7008883-b572-46db-956f-04c3c6bdb67e> ;
        foo:identifierDramaQueen  "6eeb3786-43b1-491f-bb1c-c5ade3b941a2"^^xsd:string ;
