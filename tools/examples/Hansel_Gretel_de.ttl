<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/f7008883-b572-46db-956f-04c3c6bdb67e>
        a                         foo:Scene ;
        foo:dayTime               "Tag"^^xsd:string ;
        foo:identifierDramaQueen  "f7008883-b572-46db-956f-04c3c6bdb67e"^^xsd:string ;
        foo:interiorExterior      "intext"^^xsd:string ;
        foo:internalIdentifier    "67"^^xsd:long ;
        foo:sceneContent          "<p>Die Hexe lädt mehr und mehr Essen auf Hänsels Tablett. Sie bringt es ihm jetzt selbst. Hänsel hat bereits viel an Gewicht zugenommen. Gretel sieht von weitem zu.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Die Hexe erhöht Hänsels Essensration."^^xsd:string ;
        foo:sceneHeader           "<p>Hexenhaus\tInnen/Außen/Tag</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "48"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/e27a65ca-9bde-4fa9-84aa-8285b28932b2> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/d5b1d8e6-bbdc-42d8-93a1-e01cd60c40fa>
        a                         foo:Scene ;
        foo:dayTime               "Tag"^^xsd:string ;
        foo:identifierDramaQueen  "d5b1d8e6-bbdc-42d8-93a1-e01cd60c40fa"^^xsd:string ;
        foo:interiorExterior      "int"^^xsd:string ;
        foo:internalIdentifier    "93"^^xsd:long ;
        foo:sceneContent          "<p>Die Kinder und der Vater sitzen am Frühstückstisch und essen Haferbrei. Die Mutter ist nicht da. </p><p></p><p>VATER</p><p>Das ist doch richtig gut gelaufen. Nicht wahr? Ich wusste, du wirst den Weg zurück finden, Hänsel. Du bist immerhin mein Sohn! So - seid ihr bereit für die zweite Runde?</p><p></p><p>Gretel ist sich nicht sicher. Hänsel spielt wieder den Starken für beide.</p><p></p><p>HÄNSEL</p><p>Na klar! Wir sind in Null Komma Nix wieder da!</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Der Vater unternimmt einen zweiten Versuch, die Kinder auszusetzen."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Tag</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "22"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/c34ef9bc-1516-4444-8f11-23a234564a4a> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/937dd8e8-ea25-45dd-a412-8aa928c18c05>
        a                         foo:Scene ;
        foo:dayTime               "Nacht"^^xsd:string ;
        foo:identifierDramaQueen  "937dd8e8-ea25-45dd-a412-8aa928c18c05"^^xsd:string ;
        foo:interiorExterior      "int"^^xsd:string ;
        foo:internalIdentifier    "80"^^xsd:long ;
        foo:sceneContent          "<p>Hänsel und Gretel schlafen inmitten der Taschen. Hänsel träumt.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Hänsel träumt im Schlaf:"^^xsd:string ;
        foo:sceneHeader           "<p>Hexenhaus\tInnen/Nacht</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "57"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/e27a65ca-9bde-4fa9-84aa-8285b28932b2> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/SceneGroup/7f8a6797-8943-479f-9d73-76372fbcb7cc>
