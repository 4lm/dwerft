        foo:title                 "Hansel_Gretel_de.dq"^^xsd:string .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Character/3a2cb853-e5ab-4a28-94b5-2b1e867a8d9a>
        a                         foo:Character ;
        foo:age                   "9"^^xsd:int ;
        foo:aliasName             "Schwester, Schwesterherz"^^xsd:string ;
        foo:appearance            "zierlich, blond"^^xsd:string ;
        foo:characterDescription  "<p>Gretel ist das jüngere Kind. Sie liebt ihre Familie - sogar ihre kalte und distanzierte Mutter.  Auch wenn sie weiß, dass die Familie kein Geld hat, kann sie ihren Traum von einem besseren Leben wie im Märchen nicht aufgeben. </p><p></p><p>      </p>"^^xsd:string ;
        foo:fullName              "Gretel Emily Holzmann"^^xsd:string ;
        foo:identifierDramaQueen  "3a2cb853-e5ab-4a28-94b5-2b1e867a8d9a"^^xsd:string ;
        foo:internalIdentifier    "3"^^xsd:long ;
        foo:name                  "Gretel"^^xsd:string ;
        foo:sex                   "weiblich"^^xsd:string .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/58059196-ddb7-4985-bedb-6fd0a3d5a898>
        a                         foo:Scene ;
        foo:dayTime               "Nacht"^^xsd:string ;
        foo:identifierDramaQueen  "58059196-ddb7-4985-bedb-6fd0a3d5a898"^^xsd:string ;
        foo:interiorExterior      "int"^^xsd:string ;
        foo:internalIdentifier    "40"^^xsd:long ;
        foo:sceneContent          "<p>Gretel stürmt ins Haus und findet ihre Eltern in Tränen aufgelöst vor - sie umarmt beide.</p><p></p><p>GRETEL</p><p>Das war vielleicht ein Spaß! Aber wir haben es zurück geschafft. Wir haben es geschafft.</p><p></p><p>Hänsel beobachtet die Reaktion seiner Eltern von der Tür aus und sieht, wie sie sich von Schock zu gespielter Freude verwandeln. Gretel bekommt davon nichts mit - sie ist einfach nur zu froh.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Gretel überrascht ihre Eltern mit ihrer Rückkehr."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Nacht</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "20"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/c34ef9bc-1516-4444-8f11-23a234564a4a> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/70173b5c-eb3f-467f-b727-9e6942387d6e>
        a                         foo:Scene ;
        foo:dayTime               "Früher Morgen"^^xsd:string ;
        foo:identifierDramaQueen  "70173b5c-eb3f-467f-b727-9e6942387d6e"^^xsd:string ;
        foo:interiorExterior      "intext"^^xsd:string ;
        foo:internalIdentifier    "92"^^xsd:long ;
        foo:sceneContent          "<p>Hänsel kann nicht schlafen. Gretel ist im Tiefschlaf.</p><p></p><p>Hänsel schleicht wieder zur Schlafzimmertür seiner Eltern. Die Mutter ist außer sich vor Wut.</p><p></p><p>MUTTER (OFF)</p><p>Erzähl mir nicht, du hättest nicht gemerkt, was er getan hat! Wie hättest du das nicht bemerken können! Spiel hier nicht den Dummen!</p><p></p><p>Sie stürzt auf die Tür zu. Hänsel springt hinter den Tisch, gerade als die Zimmertür aufgerissen wird und die Mutter aus dem Haus rennt. Der Vater folgt ihr nicht.</p><p></p><p>Das Geräusch von Steinen, die im Wasser landen. Hänsel schleicht zum Fenster und späht hinaus. Die Mutter sammelt alle weißen Kieselsteine auf und wirft sie in den Brunnen.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Hänsel hört wieder den Streit seiner Eltern mit."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Außen/Früher Morgen</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "21"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/c34ef9bc-1516-4444-8f11-23a234564a4a> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/5b8384a1-1644-47e8-8316-33005ac38a6f>
        a                         foo:Scene ;
        foo:dayTime               "Tag"^^xsd:string ;
        foo:identifierDramaQueen  "5b8384a1-1644-47e8-8316-33005ac38a6f"^^xsd:string ;
        foo:interiorExterior      "intext"^^xsd:string ;
        foo:internalIdentifier    "89"^^xsd:long ;
        foo:sceneContent          "<p>Gretel sitzt am Tisch und malt Märchenbilder. Die Wände sind bereits voll von ihren und des Vaters Bildern. Das Haus ist immer noch einfach eingerichtet, aber es strahlt jetzt eine Wärme und Liebe aus, die es vorher nicht hatte.</p><p></p><p>Gretel geht raus und findet Hänsel und den Vater beim Holzhacken.</p><p></p><p>GRETEL</p><p>Pizza heute Abend?</p><p></p><p>HÄNSEL</p><p>Ich helfe dir. Ich möcht meine mit grüner Paprika und Zwiebeln. Papa? Tomaten und Käse?</p><p></p><p>VATER</p><p>Wie immer.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Sie führen fortan ein glückliches Leben."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Außen/Tag</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "65"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/c34ef9bc-1516-4444-8f11-23a234564a4a> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/SceneGroup/08d356e5-2ab9-4e83-b0de-8cb620edf0d3>
        a                         foo:SceneGroup ;
        foo:hasScene              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/c47c49e4-853d-4033-9aea-0df77dd2b91a> , <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/ef35ea28-08a3-4102-878a-84025e561c9d> ;
        foo:identifierDramaQueen  "08d356e5-2ab9-4e83-b0de-8cb620edf0d3"^^xsd:string ;
        foo:internalIdentifier    "75"^^xsd:long .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/bc7f7a86-8f33-4a90-b32c-33786ace9fa4>
        a                         foo:Scene ;
        foo:dayTime               "Tag"^^xsd:string ;
        foo:identifierDramaQueen  "bc7f7a86-8f33-4a90-b32c-33786ace9fa4"^^xsd:string ;
        foo:interiorExterior      "ext"^^xsd:string ;
        foo:internalIdentifier    "62"^^xsd:long ;
        foo:sceneContent          "<p>Gretel bringt ihm wieder ein Tablett - noch beladener als das am Vorabend. Hänsel kann sein Glück kaum fassen - soviel Essen. Und so lecker!</p><p></p><p>Gretel schaut sehr angespannt, während er es herunterschlingt.</p><p></p><p>GRETEL</p><p>(sehr leise)</p><p>Hänsel, nicht... Iss nur ein bisschen.</p><p></p><p>Hänsel versteht nicht.</p><p></p><p>GRETEL</p><p>(etwas lauter)</p><p>Iss nicht alles auf. Sie will...</p><p></p><p>HEXE</p><p>Nur das Essen - keine Unterhaltung. Komm, meine Liebe. Zack, zack. Zurück ins Haus.</p><p></p><p>Hänsel bleibt verwirrt zurück.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Gretel versucht vergeblich, Hänsel zu warnen."^^xsd:string ;
        foo:sceneHeader           "<p>Hexenhaus\tAußen/Tag</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "41"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/e27a65ca-9bde-4fa9-84aa-8285b28932b2> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/c5d8a414-e6d3-4b03-aae6-919c19cb3026>
        a                         foo:Scene ;
        foo:dayTime               "Früher Morgen"^^xsd:string ;
        foo:identifierDramaQueen  "c5d8a414-e6d3-4b03-aae6-919c19cb3026"^^xsd:string ;
        foo:interiorExterior      "intext"^^xsd:string ;
        foo:internalIdentifier    "55"^^xsd:long ;
        foo:sceneContent          "<p>Während sie ihren Hunger am Haus stillen, werden sie von jemandem im Haus beobachtet.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Sie werden von der Hexe beobachtet."^^xsd:string ;
        foo:sceneHeader           "<p>Hexenhaus\tInnen/Außen/Früher Morgen</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "36"^^xsd:int ;
        foo:sceneSet              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Set/e27a65ca-9bde-4fa9-84aa-8285b28932b2> .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/63e6e6e5-c8e7-45e8-b84d-90906736e962>
        a                         foo:Scene ;
        foo:dayTime               "Tag"^^xsd:string ;
        foo:identifierDramaQueen  "63e6e6e5-c8e7-45e8-b84d-90906736e962"^^xsd:string ;
        foo:interiorExterior      "int"^^xsd:string ;
        foo:internalIdentifier    "95"^^xsd:long ;
        foo:sceneContent          "<p>Die Mutter bereit das Essen zu. Gretel tanzt um sie herum.</p><p></p><p>GRETEL </p><p>Ich liebe dieses neue Spiel! Was kochst du für uns?</p><p></p><p>MUTTER</p><p>Das wird eine Überraschung. Geh und ruh dich etwas aus für heute Nacht.</p><p></p><p>Gretel umarmt die Mutter, die von der Geste so überrascht ist, dass sie sich erst nicht bewegen kann. Aber dann legt auch sie die Arme um ihre Tochter und drückt sie an sich.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Die Mutter wird von Gretels liebevoller Umarmung überrascht."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Tag</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "24"^^xsd:int ;
