        foo:dayTime               "Nacht"^^xsd:string ;
        foo:identifierDramaQueen  "f45e6724-27f8-474c-a3bc-ee107f279ef5"^^xsd:string ;
        foo:interiorExterior      "ext"^^xsd:string ;
        foo:internalIdentifier    "83"^^xsd:long ;
        foo:sceneContent          "<p>Sie treten aus dem Hexenhaus. Und da ist ihr Stern - sie nehmen ihr Gepäck und folgen seinem Licht.</p><p>              </p>"^^xsd:string ;
        foo:sceneHeader           "<p>Hexenhaus\tAußen/Nacht</p><p>              </p>"^^xsd:string ;
