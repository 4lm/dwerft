        foo:hasScene              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/a8d70948-1361-43bc-b5f3-cf031e603413> , <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/d4758119-efbd-455e-9f71-0f84f4d776d6> , <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/9d7de2f2-b546-4043-8451-6af149d803c2> ;
        foo:identifierDramaQueen  "7f8a6797-8943-479f-9d73-76372fbcb7cc"^^xsd:string ;
