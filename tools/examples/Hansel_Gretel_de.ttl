<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/01cac583-6dd0-493a-b103-b70a5978601d>
        a                         foo:Scene ;
        foo:dayTime               "Nacht"^^xsd:string ;
        foo:identifierDramaQueen  "01cac583-6dd0-493a-b103-b70a5978601d"^^xsd:string ;
        foo:interiorExterior      "ext"^^xsd:string ;
        foo:internalIdentifier    "31"^^xsd:long ;
        foo:sceneContent          "<p>Der Vater und die Kinder laufen durch den Wald. Der Vater trägt eine Laterne.</p><p></p><p>Hänsel bleibt zurück und lässt kleine Kieselsteine auf den Weg hinter sich fallen.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Hänsel legt eine Spur aus seinen Kieselsteinen."^^xsd:string ;
        foo:sceneHeader           "<p>\tAußen/Nacht</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "13"^^xsd:int .

<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/SceneGroup/61afe56e-0dd4-4e8d-8dc1-205c31a48b16>
        a                         foo:SceneGroup ;
        foo:hasScene              <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/bc105fdf-9084-4882-b99d-a1ab67917baf> , <http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/01cac583-6dd0-493a-b103-b70a5978601d> ;
