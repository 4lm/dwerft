        foo:internalIdentifier    "81"^^xsd:long ;
        foo:sceneContent          "<p>Hänsel tritt aus dem Hexenhaus - der Wald ist hell erleuchtet. Kleine Sterne tanzen über die Dunkelheit der Bäume. Besonders ein Stern leuchtet besonders hell und umgibt Hänsel mit warmem Licht. Hänsel steigt empor und wird nach Hause getragen.</p><p>              </p>"^^xsd:string ;
        foo:sceneHeader           "<p>Traum\tAußen/Nacht</p><p>              </p>"^^xsd:string ;
        foo:sceneNumber           "58"^^xsd:int ;
