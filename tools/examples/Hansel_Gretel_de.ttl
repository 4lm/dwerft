<http://filmontology.org/resource/Project/9860f0bb-d9a6-45e4-9d03-79e7fefd16fa/Scene/e6f4b6a3-cb6a-4b2e-845b-22b0c3c36bbf>
        a                         foo:Scene ;
        foo:dayTime               "Früher Morgen"^^xsd:string ;
        foo:identifierDramaQueen  "e6f4b6a3-cb6a-4b2e-845b-22b0c3c36bbf"^^xsd:string ;
        foo:interiorExterior      "ext"^^xsd:string ;
        foo:internalIdentifier    "70"^^xsd:long ;
        foo:sceneContent          "<p>Die Jahreszeit hat gewechselt.</p><p></p><p>Hänsel ist ihr immer noch zu dünn und die Hexe rastet aus und stürmt zurück ins Hause.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Monate später verliert die Hexe endgültig die Geduld."^^xsd:string ;
