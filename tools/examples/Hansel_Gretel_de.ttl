        foo:sceneContent          "<p>Hänsel sieht Licht zwischen den Bäumen und rennt darauf zu. Gretel folgt ihm.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Sie entdecken ein Haus im Wald."^^xsd:string ;
