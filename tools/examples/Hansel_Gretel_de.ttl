        foo:identifierDramaQueen  "9f12215a-a92f-4cfb-a6d4-4d5869bb023f"^^xsd:string ;
        foo:interiorExterior      "int"^^xsd:string ;
        foo:internalIdentifier    "99"^^xsd:long ;
        foo:sceneContent          "<p>Der Vater sitzt am Tisch und versucht die Zeitung zu lesen, während die Mutter den Abwasch macht. Wieder haut sie laut mit den Töpfen umher. Der Vater steht auf.</p><p></p><p>VATER</p><p>Lass mich dir doch helfen.</p><p></p><p>MUTTER</p><p>Lass. Es geht schneller, wenn ich es allein mache!</p><p></p><p>Er versucht, den Arm um sie zu legen.</p><p></p><p>VATER</p><p>Es ist OK. Ich vermisse sie doch auch.</p><p></p><p>Die Mutter antwortet nicht aber legt auch ihre Arme um ihren Mann. Ihre Nähe tröstet ihn. Ihr verbittertes Gesicht bleibt wie es ist.</p><p>              </p>"^^xsd:string ;
        foo:sceneDescription      "Der Vater missversteht die Laune der Mutter."^^xsd:string ;
        foo:sceneHeader           "<p>Hänsels und Gretels Zuhause\tInnen/Früher Morgen</p><p>              </p>"^^xsd:string ;
